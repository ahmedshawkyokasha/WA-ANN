%% Data normalization

climate_data = readtable('climate data.csv');

% Extract precipitation and temperature data
prcp = data.Precipitation;
tg = data.Temperature;

% Z-score normalization

prcp_standardized = (prcp - mean(prcp)) ./ std(prcp);
tg_standardized = (tg - mean(tg)) ./ std(tg);

% Save the standardized data

csvwrite('standardized precipitation.csv', prcp_standardized);
csvwrite('standardized temperature.csv', tg_standardized);
